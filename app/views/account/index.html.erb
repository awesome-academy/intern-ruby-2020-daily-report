<%= provide :title, @user.name %>
<h2 class="title-center col-md-12 box-show">
  <%= t ".header" %>
</h2>
<div class="col-md-9 box-show box-account-info">
  <%= render "shared/account_info", user: @user, division: @division %>
  <div class="col-md-12 btn-to-edit-account">
    <%= link_to t(".edit"), edit_account_path(current_user), class: "btn btn-info", role: "button" %>
  </div>
</div>
