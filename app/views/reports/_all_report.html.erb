<div>
  <h4>
    <%= t ".all_report" %>
    (<span id="num-reports">
      <%= num_of_reports %>
    </span>)
  </h4>
</div>
<table class="table table-hover">
  <thead>
    <tr>
      <th><%= t ".id" %></th>
      <th><%= t ".create_at" %></th>
      <th><%= t ".update_at" %></th>
      <th><%= t ".status" %></th>
      <th><%= t ".action" %></th>
    </tr>
  </thead>
  <tbody id="tbody-all-report">
    <%= render "report_table", reports: reports %>
  </tbody>
</table>
<%= paginate reports %>
