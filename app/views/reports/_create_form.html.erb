<%= form_for report do |f| %>
  <%= render "shared/error_messages", object: f.object %>
  <div class="form-group">
    <%= f.label :today_plan %>
    <span class="require-field">*</span>
    <%= f.text_area :today_plan, class: "form-control", row: Settings.form.num_row %>
  </div>
  <div class="form-group">
    <%= f.label :actual %>
    <span class="require-field">*</span>
    <%= f.text_area :actual, class: "form-control", row: Settings.form.num_row %>
  </div>
  <div class="form-group">
    <%= f.label :reason_not_complete %>
    <%= f.text_area :reason_not_complete, class: "form-control", row: Settings.form.num_row %>
  </div>
  <div class="form-group">
    <%= f.label :tomorrow_plan %>
    <span class="require-field">*</span>
    <%= f.text_area :tomorrow_plan, class: "form-control", row: Settings.form.num_row %>
  </div>
  <div class="form-group">
    <%= f.label :free_comment %>
    <%= f.text_area :free_comment, class: "form-control", row: Settings.form.num_row %>
  </div>
  <%= f.submit t(".create"), class: "btn btn-primary" %>
<% end %>
